ifeq ($(REGEXP), PCREParser)
ADDITIONAL_OBJCFLAGS += -DUSE_PCREParser
ADDITIONAL_INCLUDE_DIRS += -IPCREParser
ADDITIONAL_LIB_DIRS += -LPCREParser
REGEXP_ADDITIONAL_LIBRARIES = -lPCREParser -lpcre
else
ADDITIONAL_OBJCFLAGS += -DUSE_OgreKit
ADDITIONAL_INCLUDE_DIRS += -I/$(PROJECT_DIR)/OgreKit
ADDITIONAL_LIB_DIRS += -L$(PROJECT_DIR)/../OgreKit/$(GNUSTEP_OBJ_DIR)
REGEXP_ADDITIONAL_LIBRARIES = -lOgreKit
endif


